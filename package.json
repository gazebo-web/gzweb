{
  "name": "gzweb",
  "version": "3.0.1",
  "description": "A library for Gazebo and data visualization.",
  "type": "module",
  "engines": {
    "node": "24",
    "npm": "11"
  },
  "main": "dist/gzweb.js",
  "module": "./dist/gzweb.module.js",
  "exports": {
    ".": {
      "import": "./dist/gzweb.module.js"
    },
    "./src/*": "./src/*"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gazebo-web/gzweb.git"
  },
  "files": [
    "dist/gzweb.js",
    "dist/gzweb.min.js",
    "dist/gzweb.module.js",
    "package.json",
    "index.js",
    "READEME.md",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && tsc && rollup -c",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "eventemitter2": "^6.4.5",
    "fast-xml-parser": "^4.1.3",
    "jszip": "^3.10.0",
    "protobufjs": "^6.11.3",
    "rxjs": "^7.5.5",
    "three": "^0.141.0",
    "three-nebula": "^10.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^28.1.1",
    "@types/three": "^0.141.0",
    "husky": "^9.1.7",
    "jest": "^28.1.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.7.3"
  }
}
